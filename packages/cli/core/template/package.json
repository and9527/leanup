{
  "name": "@template/core",
  "version": "1.1.0-rc.208",
  "description": "",
  "scripts": {
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "start": "npm run serve -- --open",
    "update": "npm install --legacy-peer-deps && npm update --legacy-peer-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@leanup/form": "^1.1.0-rc.207",
    "@leanup/lib": "^1.1.0-rc.207",
    "navigo": "^7.1.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@leanup/cli": "^1.1.0-rc.207",
    "@leanup/cli-addons": "^1.1.0-rc.207",
    "@leanup/cli-cucumber": "^1.1.0-rc.207",
    "@leanup/git-hooks": "^1.1.0-rc.207",
    "@leanup/stack": "^1.1.0-rc.208",
    "chromedriver": "^87.0.0",
    "geckodriver": "^1.21.0",
    "selenium-server": "^3.141.59",
    "typescript": "^4.1.2"
  }
}
