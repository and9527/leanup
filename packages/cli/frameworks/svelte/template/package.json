{
  "name": "@template/svelte",
  "version": "1.1.0-rc.181",
  "description": "",
  "scripts": {
    "e2e-test": "mkdirp .reports && cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber.json",
    "build": "svelte build",
    "coverage": "svelte coverage",
    "e2e": "svelte e2e",
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "serve": "svelte serve",
    "start": "npm run serve -- --open",
    "test": "svelte test",
    "update": "npm install && npm update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@leanup/form": "^1.1.0-rc.174",
    "@leanup/lib": "^1.1.0-rc.174",
    "navigo": "^7.1.2",
    "svelte": "^3.29.7"
  },
  "devDependencies": {
    "@leanup/cli": "^1.1.0-rc.181",
    "@leanup/cli-addons": "^1.1.0-rc.181",
    "@leanup/cli-cucumber": "^1.1.0-rc.181",
    "@leanup/cli-svelte": "^1.1.0-rc.181",
    "@leanup/git-hooks": "^1.1.0-rc.174",
    "@pyoner/svelte-types": "^3.4.4-2",
    "chromedriver": "^87.0.0",
    "geckodriver": "^1.21.0",
    "selenium-server": "^3.141.59",
    "typescript": "^4.1.2"
  },
  "gitHead": "d77d4ffcfdbaddda7b135c14a94a809981d488a6"
}
